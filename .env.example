# .env.example

# DrChrono API OAuth2 access token (required)
DRCHRONO_ACCESS_TOKEN=your_drchrono_access_token

# Base URL for DrChrono API (optional, default is https://drchrono.com/api/)
DRCHRONO_API_BASE=https://drchrono.com/api/

# Directory to save matching PDFs (optional, default is /tmp/matched_pdfs/)
MATCHED_PDFS_PATH=/tmp/matched_pdfs/

# Provider identification string to search for in the PDF (required)
PROVIDER_STRING="Dr. John Doe"

# AWS S3 configuration
S3_BUCKET=your-s3-bucket-name
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_REGION=us-east-1
